{
  "JwtSettings": {    
    "Issuer": "Lunora",     
    "Audience": "Lunora-Users"   
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "will be overridden by DATABASE_URL env var"
  },
  "AppSettings": {
    "AppName": "Lunora",
    "Version": "1.0.0",
    "EnableDetailedErrors": false,
    "EnableSwagger": false
  },
  "Cors": {
    "AllowedOrigins": [
      "https://lunora.netlify.app"
    ]
  }
}